      crossorigin="anonymous"
    ></script>
    <script>
      dataSets = <%- dataSets %>
      labels = <%- labels %>
    </script>
    <div
      class="chart-container"
      style="position: relative; height: 40vh; width: 80vw"
    >
      <canvas id="myChart" width="800" height="300"></canvas>
    </div>
    <script src="static/index.js"></script>
  </body>
</html>
